<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>IterationEins</servlet-name>
        <servlet-class>at.htlpinkafeld.generator.IterationEins</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IterationZwei</servlet-name>
        <servlet-class>at.htlpinkafeld.generator.IterationZwei</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IterationDrei</servlet-name>
        <servlet-class>at.htlpinkafeld.generator.IterationDrei</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IterationEins</servlet-name>
        <url-pattern>/IterationEins</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IterationZwei</servlet-name>
        <url-pattern>/IterationZwei</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IterationDrei</servlet-name>
        <url-pattern>/IterationDrei</url-pattern>
    </servlet-mapping>
</web-app>